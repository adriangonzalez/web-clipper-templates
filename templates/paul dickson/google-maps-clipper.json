{
	"schemaVersion": "0.1.0",
	"name": "Google Maps",
	"behavior": "create",
	"noteContentFormat": "",
	"properties": [
		{
			"name": "Date-Created",
			"value": "{{date}}",
			"type": "text"
		},
		{
			"name": "tags",
			"value": "My/WebClippings, #Places, #to-visit",
			"type": "multitext"
		},
		{
			"name": "Category",
			"value": "[[Places]]",
			"type": "text"
		},
		{
			"name": "Type",
			"value": "",
			"type": "text"
		},
		{
			"name": "Source",
			"value": "{{url}}",
			"type": "text"
		},
		{
			"name": "Location",
			"value": "{{url|split:\\\"/@\\\"|slice:1|split:\\\",\\\"|slice:0,2}}",
			"type": "text"
		},
		{
			"name": "Address",
			"value": "{{selector:button[aria-label^=\\\"Address:\\\"]:aria-label|split:\\\"Address: \\\"|slice:1}}",
			"type": "text"
		},
		{
			"name": "URL",
			"value": "{{selector:div[aria-label^=\\\"Information for\\\"] a[href*=\\\"http\\\"]:href|slice:0,1}}",
			"type": "multitext"
		},
		{
			"name": "Rating",
			"value": "",
			"type": "multitext"
		},
		{
			"name": "scoreGoogle",
			"value": "{{selector:div[jsaction*=\\\"moreReviews\\\"] .fontDisplayLarge}}",
			"type": "number"
		},
		{
			"name": "Description",
			"value": "{{selector:div[aria-label^=\\\"About \\\"] button .PYvSYb[jslog]}}",
			"type": "text"
		}
	],
	"triggers": [
		"https://www.google.com/maps/place/"
	],
	"noteNameFormat": "{{selector:div[role=\"main\"]:aria-label}}",
	"path": "04 VAULT/Web Clippings"
}